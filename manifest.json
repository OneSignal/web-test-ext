{
  "manifest_version": 2,
  "name": "OneSignal Web SDK Tests Extension",
  "description": "Extends web SDK test functionality by automating Chrome browser actions and overriding site notification permissions.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "assets/icon-enabled.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["script.js"]
  },
  "icons": {
    "256": "assets/icon256.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contentSettings",
    "https://webpushtest:3001/*",
    "http://webpushtest:3000/*",
    "https://nevada:3001/*",
    "https://oregon:3001/*",
    "http://washington:3000/*",
    "https://127.0.0.1:3001/*",
    "https://*.localhost:3001/*"
  ],
  "key": "",
  "externally_connectable": {
    "matches": ["https://webpushtest:3001/*", "http://webpushtest:3000/*"]
  }
}